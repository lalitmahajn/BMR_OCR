{
    "page_type": "DEVIATION_ACCEPTANCE",
    "document_metadata": {
        "document_type": "DEVIATION ACCEPTANCE",
        "company_name": "RISHABH METALS & CHEMICALS PVT. LTD.",
        "document_title": "ACCEPTANCE UNDER DEVIATION FOR RAW MATERIAL/ FINISHED PRODUCTS/PACKING MATERIAL",
        "form_no": "RR-14-04, Rev: 00",
        "stamp": "MASTER COPY"
    },
    "extraction_template": {
        "header_fields": {
            "NAME_OF_MATERIAL": {
                "label": "Name of RM/FP/PM",
                "regex": "Name\\s*of\\s*RM/?FP/?PM\\s*[:\\-]?\\s*([^\\n|]+)",
                "type": "string"
            },
            "SUPPLIER_LOT_BATCH_NO": {
                "label": "Supplier Lot No. /Batch No",
                "regex": "Supplier\\s*Lot\\s*No\\.?\\s*/?\\s*Batch\\s*No\\.?\\s*[:\\-]?\\s*([^\\n|]+?)(?=\\s*(?:Quantity|$))",
                "type": "string"
            },
            "QUANTITY": {
                "label": "Quantity",
                "regex": "Quantity\\s*[:\\-]?\\s*([\\d,]+\\s*(?:kg|KGS|Kgs|Nos|Ltrs)?)",
                "type": "string"
            },
            "IMI_AR_NO": {
                "label": "IMI/ A.R No",
                "regex": "IMI/?\\s*A\\.?R\\.?\\s*No\\.?\\s*[:\\-]?\\s*([A-Z0-9/\\-]+?)(?=\\s*(?:Vendor|$))",
                "type": "string"
            },
            "VENDOR_NAME": {
                "label": "Vendor/Supplier Name",
                "regex": "Vendor/?Supplier\\s*Name\\s*[:\\-]?\\s*([^\\n|]+)",
                "type": "string"
            },
            "DATE_OF_TESTING": {
                "label": "Date of Testing",
                "regex": "Date\\s*of\\s*Testing\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{2,4})",
                "type": "date"
            }
        },
        "test_parameters_table": [],
        "table_config": {
            "parameter_column_keywords": [
                "Parameters",
                "Parameter"
            ],
            "result_column_keywords": [
                "Test Results",
                "Results"
            ],
            "extract_all_columns": true,
            "dynamic_rows": true,
            "column_mapping": {
                "parameters": [
                    "Parameters"
                ],
                "std_specification": [
                    "Std.Specification",
                    "Std. Specification"
                ],
                "inhouse_specification": [
                    "Inhouse RM specification",
                    "Inhouse Specification"
                ],
                "test_results": [
                    "Test Results"
                ],
                "remarks": [
                    "Remarks"
                ]
            },
            "noise_markers": [
                "Kindly permit",
                "Approved By",
                "Signature",
                "Preapproval By",
                "Form No",
                "CONTROLLED COPY",
                "Copy No",
                "Issued by"
            ]
        },
        "footer_fields": {
            "DEVIATION_STATEMENT": {
                "label": "Kindly permit acceptance of the RM/FP/PM materials under deviation in respect of the above properties.",
                "regex": "Kindly\\s*permit\\s*acceptance[\\s\\S]*?properties",
                "type": "string"
            },
            "APPROVAL_QCM": {
                "label": "Approved By: QCM",
                "type": "signature"
            },
            "QCM_DATE": {
                "label": "Date",
                "regex": "Approved\\s*By\\s*:\\s*QCM[\\s\\S]*?Date\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{2,4})",
                "type": "date"
            },
            "PREAPPROVAL_QAM": {
                "label": "Preapproval By : QAM",
                "type": "signature"
            },
            "QAM_DATE": {
                "label": "Date",
                "regex": "Preapproval\\s*By\\s*:\\s*QAM[\\s\\S]*?Date\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{2,4})",
                "type": "date"
            },
            "APPROVAL_ED": {
                "label": "Approved By: ED",
                "type": "signature"
            },
            "ED_DATE": {
                "label": "Date",
                "regex": "Approved\\s*By\\s*:\\s*ED[\\s\\S]*?Date\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{2,4})",
                "type": "date"
            }
        }
