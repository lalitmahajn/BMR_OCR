{
    "page_type": "PACKING_DETAILS",
    "document_metadata": {
        "document_type": "PACKING DETAILS",
        "company_name": "RISHABH METALS & CHEMICALS PVT. LTD.",
        "document_title": "Packing Details",
        "stamp": "MASTER COPY",
        "ref_bmr_no": "RR-08-14",
        "pagination_pattern": "Page\\s*No\\.-(\\d+)\\s*of\\s*(\\d+)"
    },
    "extraction_template": {
        "header_fields": {
            "PRODUCT_NAME": {
                "label": "Name of Product :",
                "regex": "Name\\s*of\\s*Product\\s*[:\\-]?\\s*([^\\n|]+)",
                "type": "string"
            },
            "BATCH_NO": {
                "label": "Batch No. :",
                "regex": "Batch\\s*No\\.?\\s*[:\\-]?\\s*([^\\n|]+)",
                "type": "string"
            },
            "TOTAL_QTY": {
                "label": "Total Qty:",
                "regex": "Total\\s*Qty\\s*[:\\-]?\\s*([^\\n|]+)",
                "type": "string"
            },
            "TARE_WT": {
                "label": "Tare Wet.:",
                "regex": "Tare\\s*Wet\\.?\\s*[:\\-]?\\s*([^\\n|]+)",
                "type": "string"
            },
            "BALANCE_ID": {
                "label": "Balance ID.No. :",
                "regex": "Balance\\s*ID\\.?\\s*No\\.?\\s*[:\\-]?\\s*([^\\n|]+)",
                "type": "string"
            },
            "CALIBRATION_STATUS": {
                "label": "Calibration status:",
                "regex": "Calibration\\s*status\\s*[:\\-]?\\s*([^\\n|]+)",
                "type": "string"
            },
            "RINSING_STATUS": {
                "label": "All Carboy's/Barrels/IBC Rinsed (Yes / No):",
                "regex": "Rinsed\\s*\\(?Yes\\s*/\\s*No\\)?\\s*[:\\-]?\\s*([^\\n|]+)",
                "type": "string"
            },
            "SIGNATURE_DATE": {
                "label": "Signature and Date:",
                "regex": "Signature\\s*and\\s*Date\\s*[:\\-]?\\s*([^\\n|]+)",
                "type": "signature"
            },
            "PAGE_INFO": {
                "label": "Page No.",
                "regex": "Page\\s*No\\.-(\\d+\\s*of\\s*\\d+)",
                "type": "string"
            }
        },
        "table_config": {
            "extract_all_columns": true,
            "dynamic_rows": true,
            "multi_page": true,
            "calculation_rule": "Net wet = Gross wet - Tare Wet",
            "column_mapping": {
                "drum_no": [
                    "Drum No."
                ],
                "gross_wet": [
                    "Gross Wet.",
                    "Gross Wgt."
                ],
                "done_by": [
                    "Done By"
                ],
                "checked_by": [
                    "Checked By"
                ]
            },
            "noise_markers": [
                "CONTROLLED COPY",
                "Copy No.",
                "Issued by",
                "Net wet"
            ],
            "checked_by_note": "One common checked by sign from drum No. 01 to 10 and after same till end"
        },
        "noise_markers": [
            "MASTER COPY",
            "CONTROLLED COPY",
            "Copy No.",
            "Issued by"
        ]
    }
}