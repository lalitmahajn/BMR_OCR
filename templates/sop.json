{
    "page_type": "SOP",
    "document_metadata": {
        "document_type": "STANDARD OPERATING PROCEDURE",
        "company_name": "RISHABH METALS AND CHEMICALS PVT. LTD.",
        "document_title": "STANDARD OPERATING PROCEDURE",
        "stamp": "MASTER COPY",
        "pagination_pattern": "Page\\s*(\\d+)\\s*of\\s*(\\d+)"
    },
    "extraction_template": {
        "header_fields": {
            "SOP_NO": {
                "label": "SOP NO.:",
                "regex": "SOP\\s*NO\\.?\\s*[:\\-]?\\s*\\|?\\s*([^\\n|]+?)(?=\\s*(?:REVISION|\\||$))",
                "type": "string"
            },
            "REVISION": {
                "label": "REVISION:",
                "regex": "REVISION\\s*[:\\-]?\\s*\\|?\\s*(\\d+)",
                "type": "string"
            },
            "PRODUCT_NAME": {
                "label": "NAME OF PRODUCT:",
                "regex": "NAME\\s*OF\\s*PRODUCT\\s*[:\\-]?\\s*\\|?\\s*([^\\n|]+)",
                "type": "string"
            },
            "EFFECTIVE_DATE": {
                "label": "EFFECTIVE DATE:",
                "regex": "EFFECTIVE\\s*DATE\\s*[:\\-]?\\s*\\|?\\s*(\\d{2}/\\d{2}/\\d{2,4})",
                "type": "date"
            },
            "PAGE_INFO": {
                "label": "Page",
                "regex": "Page\\s*\\|?\\s*(\\d+\\s*of\\s*\\d+)",
                "type": "string"
            },
            "SCOPE": {
                "label": "1. SCOPE",
                "regex": "SCOPE\\s*[:\\-]?\\s*\\|?\\s*([^\\n]+(?:Product\\s*\\d+))",
                "type": "string"
            },
            "IMPORTANT_NOTES": {
                "label": "2.1 IMPORTANT NOTES",
                "regex": "(?s)IMPORTANT\\s*NOTES\\s*(.*?)(?=2\\.2|PRE\\s*CHECKS|$)",
                "type": "text_block",
                "multi_page": true,
                "continuation_regex": "(?s)^\\s*((?:\\d+\\)|\\*|\\-|\\d+\\.)\\s+.*?)(?=PREPARED|CHECKED|APPROVED|$)"
            },
            "PRE_CHECKS": {
                "label": "2.2 PRE CHECKS",
                "regex": "(?s)PRE\\s*CHECKS\\s*(.*?)(?=2\\.3|PRE\\s*OPERATING|PREPARED|$)",
                "type": "text_block",
                "multi_page": true,
                "continuation_regex": "(?s)^\\s*((?:\\d+\\)|\\*|\\-|\\d+\\.)\\s+.*?)(?=PREPARED|CHECKED|APPROVED|$)"
            },
            "PRE_OPERATING_STEPS": {
                "label": "2.3 PRE OPERATING STEPS",
                "regex": "(?s)PRE\\s*OPERATING\\s*STEPS\\s*(.*?)(?=3\\.|BATCH\\s*OPERATING|PREPARED|$)",
                "type": "text_block",
                "multi_page": true,
                "continuation_regex": "(?s)^\\s*((?:\\d+\\)|\\*|\\-|\\d+\\.)\\s+.*?)(?=PREPARED|CHECKED|APPROVED|$)"
            },
            "BATCH_OPERATING_PROCEDURE": {
                "label": "3.1 BATCH OPERATING PROCEDURE",
                "regex": "(?s)BATCH\\s*OPERATING\\s*PROCEDURE\\s*(.*?)(?=3\\.2|4\\.|PACKING|PREPARED|$)",
                "type": "text_block",
                "multi_page": true,
                "continuation_regex": "(?s)^\\s*((?:\\d+\\)|\\*|\\-|\\d+\\.)\\s+.*?)(?=PREPARED|CHECKED|APPROVED|$)"
            },
            "PACKING_MATERIAL": {
                "label": "4. PACKING OF MATERIAL",
                "regex": "(?s)PACKING\\s*OF\\s*MATERIAL\\s*(.*?)(?=5\\.|RESPONSIBILITY|PREPARED|$)",
                "type": "text_block",
                "multi_page": true,
                "continuation_regex": "(?s)^\\s*((?:\\d+\\)|\\*|\\-|\\d+\\.)\\s+.*?)(?=PREPARED|CHECKED|APPROVED|$)"
            }
        },
        "tables": {
